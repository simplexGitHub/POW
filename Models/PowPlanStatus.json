{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "Simplex Inventory Model Schema",
  "description": "Flightops inventroy and status update object",
  "type": "object",
  "title": "",
  "properties": {
    "PlanID": {
      "type": "string",
      "description": "General GUID created to identify the Plan"
    },
    "Nodes": {
      "type": "array",
      "description": "List of nodes in the plan",
      "items": {
        "$ref": "./StatusNode.json"
      }
    },
    "CreationTime": {
      "type": "string",
      "description": "Date and Time the Plan was created",
      "format": "date-time"
    },
    "LastUpdateTime": {
      "type": "string",
      "description": "Date and Time the Plan was last modified",
      "format": "date-time"
    },
    "name": {
      "type": "string"
    },
    "missionStatus": {
      "type": "string",
      "enum": [
        "Running",
        "Aborted",
        "Finished"
      ]
    },
    "PlanIdChecksum": {
      "type": "number"
    },
    "startSessionTime": {
      "$ref": "./GeneralTypes/Timestamp.json"
    },
    "stopSeessionTime": {
      "$ref": "./GeneralTypes/Timestamp.json"
    }
  },
  "required": [
    "PlanID",
    "Nodes",
    "CreationTime",
    "LastUpdateTime",
    "missionStatus"
  ]
}